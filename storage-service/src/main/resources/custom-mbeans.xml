<?xml version="1.0"?>
<mbeans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.oracle.com/coherence/coherence-operational-config"
    xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-operational-config coherence-operational-config.xsd">

    <mbean id="heap-use">
        <mbean-factory>com.oracle.coherence.examples.storage.metrics.HeapUsage</mbean-factory>
        <mbean-accessor>create</mbean-accessor>
        <mbean-name>type=HeapUsage</mbean-name>
        <enabled>true</enabled>
    </mbean>
</mbeans>